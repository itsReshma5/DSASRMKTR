#include <stdio.h>
#include<math.h>
int main()
{
 float n,i,width,height;
 scanf("%f",&n);
 int count=0;
 for(i=0;i<n;i++)
 {
 scanf("%f %f",&width,&height);
 if(width/height>=1.6 && width/height<=1.7)
 ++count;
 else if(height/width >=1.6 && height/width<=1.7)
 ++count;
 }
 printf("%d",count+1);
return 0;
}
